#: src/bin/smbios-token-ctl:397
#, c-format
msgid "       token: 0x%04x"
msgstr "       კოდი: 0x%04x"

#: src/bin/smbios-token-ctl:398 src/bin/smbios-token-ctl:415
#, c-format
msgid "        type: %s"
msgstr "        ტიპი: %s"

#: src/bin/smbios-token-ctl:399 src/bin/smbios-token-ctl:416
#, c-format
msgid "       value: %s"
msgstr "       მნიშვნელობა: %s"

#: src/bin/smbios-token-ctl:403
msgid "Setting string value ..."
msgstr "სტრიქონის ტიპის მნიშვნელობის დაყენება ..."

