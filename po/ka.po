#, c-format
msgid "Skipping '%s' due to IOError.\n"
msgstr "'%s'-ის გამოტოვება IOError-ის გამო.\n"

#: src/bin/smbios-token-ctl:169
#, c-format
msgid "The error message was: %s"
msgstr "შეცდომის შეტყობინება: %s"

