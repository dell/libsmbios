msgid "<weird unknown type>"
msgstr "<უცნაური უცნობი ტიპი>"

#: src/bin/smbios-token-ctl:177
msgid "<unknown value>"
msgstr "<უცნობი მნიშვნელობა>"

#: src/bin/smbios-token-ctl:226
#, c-format
msgid "  Token: 0x%04x - %s (%s)"
msgstr "  კოდი: 0x%04x - %s (%s)"

#: src/bin/smbios-token-ctl:230
#, c-format
msgid "  value: %s = %s"
msgstr "  მნიშვნელობა: %s = %s"

#: src/bin/smbios-token-ctl:236
msgid "   Desc: "
msgstr "    აღწ: "

#: src/bin/smbios-token-ctl:263
#, c-format
msgid "Importing token values from '%s'"
msgstr "კოდის მნიშვნელობების შემოტანა '%s' -დან"

#: src/bin/smbios-token-ctl:271
#, c-format
msgid ""
"File format error. The first line should list the column names. Could not "
"find the %s column"
msgstr ""
"ფაილის ფორმატის შეცდომა. პირველი ხაზი სვეტების სახელების სია უნდა იყოს. ვერ "
"ვიპოვე სვეტი: %s"

